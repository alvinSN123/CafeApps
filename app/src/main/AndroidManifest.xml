<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.icafebussiness.cafeapps">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.icafebussiness.cafeapps.SignUp"></activity>
        <activity android:name="com.icafebussiness.cafeapps.SignIn" />
        <activity android:name="com.icafebussiness.cafeapps.DetailCoffee" />
        <activity android:name="com.icafebussiness.cafeapps.Login" />
        <activity
            android:name="com.icafebussiness.cafeapps.SplashScreen"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.icafebussiness.cafeapps.Menu" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>